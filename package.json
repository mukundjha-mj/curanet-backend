  "main": "index.js",
  "scripts": {
    "build": "tsc -b",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build:docker": "docker build -t curanet-healthcare .",
    "watch": "tsc -w",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "db:reset": "prisma migrate reset --force",
    "db:studio": "prisma studio",
    "docker:build": "docker build -t curanet-healthcare:latest .",
    "docker:run": "docker run -p 3000:3000 --env-file .env curanet-healthcare:latest",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "clean": "rimraf dist coverage .nyc_output",
    "postman:test": "newman run postman_collection.json -e postman_environment.json",
    "security:audit": "npm audit && npm audit --audit-level moderate",
    "prepare": "husky install"
  },
